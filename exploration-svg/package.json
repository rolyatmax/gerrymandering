{
  "name": "gerrymandering-exploration-with-svg",
  "version": "1.0.0",
  "description": "an interactive look at gerrymandering",
  "scripts": {
    "start": "budo index.js:bundle.js --live --open",
    "postinstall": "cp node_modules/standard/eslintrc.json .eslintrc || true",
    "lint": "standard",
    "build": "browserify index.js > dist/bundle.js"
  },
  "author": "Taylor Baldwin <taylorbaldwin@gmail.com> (https://tbaldw.in)",
  "license": "ISC",
  "dependencies": {
    "d3": "^4.6.0",
    "dat-gui": "^0.5.0"
  },
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "budo": "^9.4.7",
    "standard": "^8.6.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ],
          "plugins": [
            "transform-object-rest-spread"
          ]
        }
      ]
    ]
  }
}
