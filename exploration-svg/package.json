{
  "name": "gerrymandering-exploration-with-svg",
  "version": "1.0.0",
  "description": "an interactive look at gerrymandering",
  "scripts": {
    "start": "serve",
    "dev": "budo index.js:bundle.js --live --open",
    "postinstall": "cp node_modules/standard/eslintrc.json .eslintrc || true",
    "lint": "standard",
    "build": "./build.sh"
  },
  "author": "Taylor Baldwin <taylorbaldwin@gmail.com> (https://tbaldw.in)",
  "license": "ISC",
  "dependencies": {
    "d3": "^4.6.0",
    "dat-gui": "^0.5.0",
    "lodash": "^4.17.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "serve": "^5.0.3",
    "yo-yo": "^1.4.0"
  },
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "budo": "^9.4.7",
    "standard": "^8.6.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ],
          "plugins": [
            "transform-object-rest-spread"
          ]
        }
      ]
    ]
  }
}
